generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "mongodb"
  url      = env("MONGODB_URI")
}

model Images {
  id        String  @id @default(auto()) @map("_id") @db.ObjectId
  title     String?
  location  String
  date      String
  page      String
  createdAt String
  imageUrl  String
}
