// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "mongodb"
  url      = env("MONGODB_URI")
}

model Images {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  title     String?
  isSmall   Boolean?
  location  String
  date      String
  page      String
  createdAt String
  imageUrl  String
}
